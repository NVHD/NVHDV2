    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.1",
    "svelte": "^3.50.1",
    "vite": "^3.1.4"
  },
  "type": "module",
  "dependencies": {
    "@sanity/client": "^3.4.1",
