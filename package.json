{
  "name": "sveltekit-sanity-template",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "build:sanity": "cd studio && npm run build",
    "start:sanity": "cd studio && npm run dev",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@beyonk/svelte-mapbox": "^8.1.4",
    "@portabletext/svelte": "1.0.0-next.1",
    "@sveltejs/adapter-netlify": "^1.0.0-next.56",
    "@sveltejs/kit": "1.0.0-next.327",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0"
  },
  "type": "module",
  "dependencies": {
    "@sanity/client": "^3.3.0",
    "@sanity/image-url": "^1.0.1",
